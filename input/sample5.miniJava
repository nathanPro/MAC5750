class Factorial {
    public static void main(String[] a) {
        System.out.println(new Fac().ComputeFac(10));
    }
}
class Fac {
    int t1;
    int t2;
    public int ComputeFac(int num) {
        int ans;
        int i;
        ans = 1;
        i = 1;
        while (!(num < i)) {
            ans = i * ans;
            i = i + 1;
        }
        return ans;
    }
}
